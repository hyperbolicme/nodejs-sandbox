<head>
        <%- include( "include/header", {title: title}   ); %>
</head>
    
<body>
    <div class="container">
        <%- include( "include/top-page", {title: title, username: username} ); %>

        <div class = "container-fluid">

        <h1 class="name"><%= user_profile.name %></h1>

            <table class = "table table-striped">
                <tr> 
                    <th>ID</th>
                    <td> <%= user_profile.student_id %> </td> 
                </tr>
                <tr> 
                    <th>Name</th>
                    <td class = "name">  <%= user_profile.name %> </td>
                </tr>
                <tr>
                    <th>Course</th> 
                    <td class = "course"> <%= user_profile.course %>  </td>
                </tr>
                <tr>
                    <th>Specialication</th> 
                    <td class = "spec"> <%= user_profile.specialization %> </td>
                </tr>
                <tr>
                    <th>Batch</th> 
                    <td> <%= user_profile.batch %> </td>
                </tr>    
                <tr>
                    <th>Phone</th> 
                    <td> <%= user_profile.phone %> </td>
                </tr>
            </table>
            <!-- <form >
                <input type="submit" class="btn btn-primary" value="Go back to search results" onClick="javascript:history.go(-1)">
            </form> -->
            
        </div>    
    </div>
</body>
